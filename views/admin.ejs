<%- include('header'); -%>

<h1>Admin/<a href="/">*</a></h1>

<div class="row">
  <div class="col s12">
    <table class="table">
      <thead>
        <tr>
          <th width="15%">Short</th>
          <th width="75%">Original</th>
          <th width="10%"></th>
        </tr>
      </thead>
      <tbody>
        <% Object.keys(originals).forEach((short) => { %>
          <tr>
            <td><%= short %></td>
            <td class="table-cell truncate"><%= originals[short] %></td>
            <td>
              <form action="/remove" method="post">
                <input type="hidden" name="short" value=<%= short %>>
                <button class="btn-remove" type="submit" name="remove">
                  Remove
                </button>
              </form>
            </td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</div>

<form action="/logout" method="post">
  <div class="row">
    <button class="btn-large waves-effect waves-light" type="submit" name="action">
      Logout
    </button>
  </div>
</form>

<%- include('footer'); -%>
